{
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "setpath-mod": "cross-env BUILD_PATH=mod",
        "setpath-cs2": "cross-env BUILD_PATH=cs2",
        "prebuild": "npm install --no-audit --no-fund",
        "build": "webpack",
        "build-dev": "webpack --mode development",
        "build-cs2": "cross-env BUILD_PATH=cs2 webpack --mode development",
        "dev": "webpack --mode development --watch",
        "dev-release": "webpack --watch",
        "dev-cs2": "cross-env BUILD_PATH=cs2 webpack --watch --env cs2",
        "tsc": "tsc",
        "update": "npx create-csii-ui-mod update",
        "clean": "npx create-csii-ui-mod clean",
        "pe": "prettier-eslint"
    },
    "exclude": [
        "node_modules",
        "build"
    ],
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "classnames": "2.3.2",
        "jsdom": "^25.0.1",
        "prettier": "^3.4.2",
        "prettier-eslint": "^16.3.0",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.17.0",
        "@types/intl": "^1.2.2",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "cross-env": "^7.0.3",
        "css-loader": "6.7.2",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-unused-imports": "^3.2.0",
        "globals": "^15.14.0",
        "mini-css-extract-plugin": "2.7.0",
        "prettier-eslint-cli": "^8.0.1",
        "sass": "^1.69.5",
        "sass-loader": "13.2.0",
        "style-loader": "^3.3.3",
        "swc-loader": "0.2.3",
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "typescript": "4.8.4",
        "webpack": "5.97.1",
        "webpack-cli": "4.10.0"
    }
}
